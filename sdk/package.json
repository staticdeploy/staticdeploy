{
  "name": "@staticdeploy/sdk",
  "description": "Browser and nodejs sdk for the StaticDeploy API",
  "version": "0.9.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "author": "Paolo Scanferla <paolo.scanferla@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/staticdeploy/staticdeploy.git"
  },
  "homepage": "https://github.com/staticdeploy/staticdeploy",
  "bugs": {
    "url": "https://github.com/staticdeploy/staticdeploy/issues"
  },
  "scripts": {
    "compile": "rimraf lib && tsc",
    "prettier": "prettier 'src/**/*.ts' 'test/**/*.ts'",
    "prettify": "yarn prettier --write",
    "lint:prettier": "yarn prettier --list-different",
    "lint:tslint": "tslint 'src/**/*.ts' 'test/**/*.ts'",
    "lint": "yarn lint:prettier && yarn lint:tslint",
    "test": "NODE_ENV=test mocha --exit -r ts-node/register -r test/setup.ts --watch-extensions ts 'test/**/*.ts'"
  },
  "dependencies": {
    "@staticdeploy/common-types": "^0.9.0",
    "axios": "^0.18.0",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/lodash": "^4.14.116",
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.3.0",
    "@types/sinon": "^5.0.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "mocha": "^5.2.0",
    "nock": "^9.4.4",
    "prettier": "^1.14.0",
    "rimraf": "^2.6.2",
    "sinon": "^6.1.4",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.0.1"
  }
}
