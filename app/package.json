{
  "name": "@staticdeploy/app",
  "description": "staticdeploy app",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "author": "Paolo Scanferla <paolo.scanferla@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/staticdeploy/staticdeploy.git"
  },
  "bugs": {
    "url": "https://github.com/staticdeploy/staticdeploy/issues"
  },
  "scripts": {
    "prettier": "prettier '@(src|test)/**/*.@(ts|tsx|css)'",
    "prettify": "yarn prettier --write",
    "lint:prettier": "yarn prettier --list-different",
    "lint:tslint": "tslint '@(src|test)/**/*.ts?(x)'",
    "lint": "npm-run-all lint:*",
    "test": "TS_NODE_TYPE_CHECK=true TS_NODE_PROJECT=test/tsconfig.json NODE_ENV=test mocha --exit -r test/setup --watch-extensions ts,tsx 'test/**/*.ts?(x)'",
    "dev:mock-server": "TS_NODE_TYPE_CHECK=true TS_NODE_PROJECT=mock-server/tsconfig.json mock-server --delay 100 --watch --require ts-node/register --serveConfig",
    "dev:server": "webpack-dev-server --config webpack/dev.js",
    "dev": "npm-run-all -p dev:*",
    "compile": "NODE_ENV=production webpack --config webpack/prod.js",
    "build": "yarn compile",
    "docker:build": "docker build -f ./Dockerfile -t staticdeploy/app:$DOCKER_TAG ../",
    "docker:push": "docker push staticdeploy/app:$DOCKER_TAG"
  },
  "dependencies": {
    "@staticdeploy/sdk": "^0.0.0",
    "antd": "^3.2.2",
    "classnames": "^2.2.5",
    "date-fns": "^1.29.0",
    "eventemitter3": "^3.0.1",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-form": "^7.2.3",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "@staticdeploy/app-server": "^1.1.1",
    "@staticdeploy/mock-server": "^1.7.0",
    "@types/chai": "^4.1.2",
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/express": "^4.11.1",
    "@types/faker": "^4.1.2",
    "@types/lodash": "^4.14.104",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.6",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.15",
    "@types/react-router-dom": "^4.2.4",
    "@types/redux-form": "^7.0.14",
    "@types/sinon": "^4.1.4",
    "@types/sinon-chai": "^2.7.29",
    "@types/validator": "^6.3.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.18",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "faker": "^4.1.0",
    "html-webpack-plugin": "^2.30.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.6.2",
    "jsdom-global": "^3.0.2",
    "lodash": "^4.17.5",
    "mocha": "^5.0.1",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.11.0",
    "sinon": "^4.4.2",
    "sinon-chai": "^2.14.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.2",
    "ts-loader": "^3.5.0",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.9.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.7.2",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.2",
    "webpack-merge": "^4.1.1"
  }
}
