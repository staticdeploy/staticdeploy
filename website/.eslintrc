{
    "parser": "babel-eslint",
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "env": { "browser": true, "commonjs": true, "node": true },
    "globals": { "process": true },
    "rules": {
        "react/display-name": false,
        "react/prop-types": false
    },
    "settings": {
        "react": { "version": "detect" }
    }
}
