{
  "name": "@staticdeploy/management-console",
  "description": "staticdeploy management console",
  "version": "0.15.4",
  "private": true,
  "main": "build/index.html",
  "files": [
    "build"
  ],
  "author": "Paolo Scanferla <paolo.scanferla@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/staticdeploy/staticdeploy.git"
  },
  "bugs": {
    "url": "https://github.com/staticdeploy/staticdeploy/issues"
  },
  "scripts": {
    "prettier": "prettier '@(src|test|mock-server)/**/*.@(ts|tsx|css)'",
    "prettify": "yarn prettier --write",
    "lint:prettier": "yarn prettier --list-different",
    "lint:tslint": "tslint '@(src|test|mock-server)/**/*.ts?(x)'",
    "lint": "npm-run-all lint:*",
    "test": "TS_NODE_PROJECT=test/tsconfig.json TS_NODE_FILES=true NODE_ENV=test mocha --exit -t 20000 -r ts-node/register -r test/setup.ts --extension ts,tsx 'test/**/*.ts?(x)'",
    "dev:mock-server": "TS_NODE_PROJECT=mock-server/tsconfig.json mock-server --delay 100 --watch --require ts-node/register --serveConfig",
    "dev:server": "react-scripts start",
    "dev": "npm-run-all -p dev:*",
    "compile": "PUBLIC_URL=. react-scripts build"
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@staticdeploy/core": "^0.15.4",
    "@staticdeploy/sdk": "^0.15.4",
    "antd": "^4.9.2",
    "classnames": "^2.2.6",
    "dayjs": "^1.9.7",
    "eventemitter3": "^4.0.7",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.20",
    "oidc-client": "^1.10.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-json-tree": "^0.13.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-form": "^8.3.7"
  },
  "devDependencies": {
    "@panva/jose": "^1.9.3",
    "@staticdeploy/mock-server": "^2.0.0",
    "@types/chai": "^4.2.14",
    "@types/classnames": "^2.2.11",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/express": "^4.17.9",
    "@types/faker": "^5.1.5",
    "@types/jsdom-global": "^3.0.2",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.12",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-json-tree": "^0.6.11",
    "@types/react-redux": "^7.1.12",
    "@types/react-router-dom": "^5.1.6",
    "@types/redux-form": "^8.3.1",
    "@types/sinon": "^9.0.9",
    "@types/sinon-chai": "^3.2.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.3.2",
    "chai": "^4.2.0",
    "enzyme": "^3.11.0",
    "faker": "^5.1.0",
    "husky": "^4.3.5",
    "ignore-styles": "^5.0.1",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "lerna": "^3.22.1",
    "mocha": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react-scripts": "^4.0.1",
    "sinon": "^9.2.2",
    "sinon-chai": "^3.4.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "typescript": "^4.1.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "import/no-anonymous-default-export": 0
    }
  }
}
