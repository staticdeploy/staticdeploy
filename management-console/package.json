    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "oidc-client": "^1.10.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-json-tree": "^0.11.2",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-form": "^8.2.6"
  },
  "devDependencies": {
    "@panva/jose": "^1.9.3",
    "@staticdeploy/mock-server": "^1.10.0",
    "@types/chai": "^4.2.7",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.9",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.5.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-json-tree": "^0.6.11",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-form": "^8.2.0",
    "@types/sinon": "^7.5.1",
    "@types/sinon-chai": "^3.2.3",
    "chai": "^4.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "faker": "^4.1.0",
    "husky": "^4.2.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^16.0.1",
    "jsdom-global": "^3.0.2",
    "lerna": "^3.20.2",
    "mocha": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react-scripts": "^3.3.0",
    "sinon": "^8.1.1",
    "sinon-chai": "^3.4.0",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.7.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
