    "faker": "^4.1.0",
    "husky": "^3.0.5",
    "ignore-styles": "^5.0.1",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "lerna": "^3.16.4",
    "mocha": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react-scripts": "^3.1.1",
    "sinon": "^7.4.2",
    "sinon-chai": "^3.3.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.6.2"
